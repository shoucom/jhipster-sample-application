<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Attendance.
    -->
    <changeSet id="20230602101732-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="employee_id"
                                 baseTableName="attendance"
                                 constraintName="fk_attendance__employee_id"
                                 referencedColumnNames="id"
                                 referencedTableName="employee"/>

        <addForeignKeyConstraint baseColumnNames="office_id"
                                 baseTableName="attendance"
                                 constraintName="fk_attendance__office_id"
                                 referencedColumnNames="id"
                                 referencedTableName="office"/>
    </changeSet>
</databaseChangeLog>
